    "ios:beta": "cd ios; bundle exec fastlane ios beta",
    "ios:alpha": "cd ios; bundle exec fastlane ios alpha",
    "ios:appstore": "cd ios; bundle exec fastlane ios appstoredeploy",
    "ios:clean": "xclean -r DerivedData",
